        function addProgram() {
          const programName = prompt("Enter the new program name:");
          if (programName) {
            const programButtons = document.getElementById("program-buttons");
            const newButton = document.createElement("button");
            newButton.className = "program-button";
            newButton.textContent = programName;
            newButton.onclick = function () {
              showSubjects(programName);
            };
            programButtons.insertBefore(
              newButton,
              programButtons.querySelector(".plus-button")
            );
          }
        }
      }
    </script>







































































